@model PuppySharpPdf.Framework.DemoProject.Models.DemoTemplateViewModel

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    @*uncomment the link below if you want to use a local css file*@
    @*<link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />*@

    @*Uncomment the link below if you want to use a online file*@
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css"/>
</head>
<body class="p-5">
    <div class="d-flex justify-content-center">
        <h1 class="text-primary">Hello World</h1>
    </div>


    <div class="card">
        <div class="card-header text-center">Pdf Generated By Razor File With Dynamic First and Last Name Values</div>
        <div class="card-body p-3">
            <p class="card-text">My name is @Model.FirstName @Model.LastName</p>
        </div>
    </div>

    <div class="mt-3">
        <div class="card w-50">
            <img src="~/Content/images/vector-image.jpg" class="card-img-top" alt="image from local content">
            <div class="card-body">
                <p class="card-text">Some quick example of an image being rendered from a local content folder.</p>
            </div>
        </div>
    </div>

    <div class="mt-3">
        <div class="card w-50">
            <img src="https://www.rd.com/wp-content/uploads/2021/01/GettyImages-1070837284-e1611613819374.jpg" class="card-img-top" alt="image from local content">
            <div class="card-body">
                <p class="card-text">Some quick example of an image being rendered from an online resource.</p>
            </div>
        </div>
    </div>
</body>
</html>